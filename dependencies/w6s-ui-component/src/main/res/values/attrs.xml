<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>


    <declare-styleable name="ForegroundView">
        <attr name="android:foreground"/>
        <attr name="android:foregroundGravity"/>
        <attr name="foregroundInsidePadding" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="PopViewItemView">
        <attr name="popIcon" format="integer"/>
        <attr name="popTitle" format="integer"/>
    </declare-styleable>

    <declare-styleable name="font_size_style">
        <attr name="fontsize_8sp" format="dimension"/>
        <attr name="fontsize_9sp" format="dimension"/>
        <attr name="fontsize_10sp" format="dimension"/>
        <attr name="fontsize_11sp" format="dimension"/>
        <attr name="fontsize_tiny_12sp" format="dimension"/>
        <attr name="fontsize_12sp" format="dimension"/>
        <attr name="fontsize_13sp" format="dimension"/>
        <attr name="fontsize_14sp" format="dimension"/>
        <attr name="fontsize_15sp" format="dimension"/>
        <attr name="fontsize_16sp" format="dimension"/>
        <attr name="fontsize_17sp" format="dimension"/>
        <attr name="fontsize_18sp" format="dimension"/>
        <attr name="fontsize_19sp" format="dimension"/>
        <attr name="fontsize_20sp" format="dimension"/>
        <attr name="fontsize_21sp" format="dimension"/>
        <attr name="fontsize_22sp" format="dimension"/>
        <attr name="fontsize_23sp" format="dimension"/>
        <attr name="fontsize_24sp" format="dimension"/>
        <attr name="fontsize_30sp" format="dimension"/>
        <attr name="fontsize_34sp" format="dimension"/>
        <attr name="fontsize_38sp" format="dimension"/>
        <attr name="fontsize_50sp" format="dimension"/>
    </declare-styleable>
</resources>